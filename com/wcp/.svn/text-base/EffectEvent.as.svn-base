package com.wcp {	import flash.events.Event;	import com.wcp.Effect;	public class EffectEvent extends Event {		public var effect:Effect;		public var effectName:Object;		public var action:String;				public static var PLAY:String = "play";		public static var STOP:String = "stop";		public static var CHANGED:String = "change";		public static var QUEUE_ADD:String = "queue_add";		public static var QUEUE_DROP:String = "queue_drop";		public static var QUEUE_CHANGE:String = "queue_change";				public function EffectEvent (_action:String="play",_effect:Effect=null,_effectName:Object=null) {			this.action = _action;			this.effect = _effect;			this.effectName = _effectName;			super("effect");		}	}}